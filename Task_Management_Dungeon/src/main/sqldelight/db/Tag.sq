CREATE TABLE Tag (
    id INTEGER NOT NULL PRIMARY KEY,
    tag TEXT NOT NULL
);

getTagById:
SELECT *
FROM Tag
WHERE id = :id;

getTagByName:
SELECT Tag.id
FROM Tag
WHERE tag = :name;

getAllTags:
SELECT *
FROM Tag;

insertTag:
INSERT OR REPLACE
INTO Tag
VALUES (?,?);

deleteTagById:
DELETE FROM Tag
WHERE id = :id;